{
    "collectCoverage": true,
    "coverageReporters": [
        "json",
        "html"
    ],
    "coverageDirectory": "coverage/unit",
    "collectCoverageFrom": [
        "src/**/*.{js,ssx}",
        "!src/index.js"
    ],
    "testRegex": "/__tests__/unit/.*\\.(test)\\.(jsx?)$",
    "moduleFileExtensions": [
        "js"
    ],
    "testPathIgnorePatterns": [
        "<rootDir>/node_modules/"
    ],
    "modulePaths": [
        "<rootDir>/src/"
    ]
}
